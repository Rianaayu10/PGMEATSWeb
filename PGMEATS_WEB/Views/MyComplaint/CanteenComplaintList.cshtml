    /*=============================================================================
     Dowmload Excel
     ==============================================================================*/
    function fnExcelReport() {

        //validasi parameter
        var ListOfMessages;
        var department = $("#Department").val();
        var issuetypeID = $("#IssueTypeID").val();
        var complaintStatus = $("#ComplaintStatus").val();

        var pfromDate = $("#fromDate").val().split(' ');
        var fromDate = pfromDate[2] + "-" + months[pfromDate[1]] + "-" + pfromDate[0]; //convert date to format "yyyy-mm-dd"

        var ptoDate = $("#toDate").val().split(' ');
        var toDate = ptoDate[2] + "-" + months[ptoDate[1]] + "-" + ptoDate[0]; //convert date to format "yyyy-mm-dd"

        //add parameter to formdata
        var formdata = new FormData();
        formdata.append("FromDate", fromDate);
        formdata.append("toDate", toDate);
        formdata.append("Department", department);
        formdata.append("IssueTypeID", issuetypeID);
        formdata.append("ComplaintStatus", complaintStatus);
        formdata.append("UserID", UserID);

        //post data
        $.ajax({
            type: "POST",
            url: "@Url.Action("ReplyComplaintList", "MyComplaint")", //"/MyComplaint/ReplyComplaintList/",
            contentType: false,
            processData: false,
            data: formdata,
            success: function (result) {
                if (result.Message.toLowerCase().includes("success")) {
                    ListOfMessages = result.Contents;
                }
                else {
                    toastr.warning(result.Message, 'Warning', { timeOut: 1000, closeButton: true });
                }
            },
            error: function (ex) {
                toastr.error(ex, 'Failed', { timeOut: 1000, "closeButton": true });
                return
            }
        }).done(function () {

            // column head table declaration
            var ColumnHead = "";
            ColumnHead += "<td><b>Date</b></td>";
            ColumnHead += "<td><b>Department</b></td>";
            ColumnHead += "<td><b>IssueType</b></td>";
            ColumnHead += "<td><b>Description</b></td>";
            ColumnHead += "<td><b>CatererID</b></td>";
            ColumnHead += "<td><b>Created By</b></td>";
            ColumnHead += "<td><b>Status</b></td>";
            ColumnHead += "<td><b>Response</b></td>";
            ColumnHead += "<td><b>Create Response</b></td>";
            ColumnHead += "<td><b>Last User</b></td>";
            ColumnHead += "<td><b>Last Update</b></td>";

             // table declaration
            var TableMarkUp = "";
            TableMarkUp += '<table>'
            TableMarkUp += '<tr><td colspan="4"><b style="font-size:160%;"> PGM EATS - List Of Feedback <b></td></tr>';
            TableMarkUp += '</table><br>'

            TableMarkUp += '<table>'
            TableMarkUp += '<tr><td colspan="2"><b>Periode<b></td><td colspan="2"><b>: ' + $("#fromDate").val() + ' - ' + $("#toDate").val() + '<b></td></tr>';
            TableMarkUp += '<tr><td colspan="2"><b>Department<b></td><td colspan="2"><b>: ' + $("#Department option:selected").text() + '<b></td></tr>';
            TableMarkUp += '<tr><td colspan="2"><b>Issue Type<b></td><td colspan="2"><b>: ' + $("#IssueTypeID option:selected").text() + '<b></td></tr>';
            TableMarkUp += '<tr><td colspan="2"><b>Status<b></td><td colspan="2"><b>: ' + $("#ComplaintStatus option:selected").text() + '<b></td></tr>';
            TableMarkUp += '</table><br><br>'

            TableMarkUp += '<table border="2px" id="myModifiedTable" class="visibilityHide"><thead><tr bgcolor="#d3d3d3">' + ColumnHead + '</tr></thead><tbody>';

            // row declaration
            for (x = 0; x < ListOfMessages.length; x++) {
                var rowdata = "";
                rowdata += '<td>' + ListOfMessages[x]["CreatedDate"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["Department"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["IssueTypeDesc"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["ComplaintDesc"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["CatererID"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["CreatedUser"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["ReplyStatus"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["ComplaintReply"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["LastUser"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["LastUser"] + '</td>'
                rowdata += '<td>' + ListOfMessages[x]["LastUpdate"] + '</td>'

                TableMarkUp += '<tr>' + rowdata + '</tr>';
            }

            TableMarkUp += "</tbody></table>";

            tab_text += '<html xmlns:x="urn:schemas-microsoft-com:office:excel">';
            tab_text += '<head><xml><x:ExcelWorkbook><x:ExcelWorksheets><x:ExcelWorksheet>';
            tab_text += '<x:Name>Error Messages</x:Name>';
            tab_text += '<x:WorksheetOptions><x:Panes></x:Panes></x:WorksheetOptions></x:ExcelWorksheet>';
            tab_text += '</x:ExcelWorksheets></x:ExcelWorkbook></xml></head><body>';
            tab_text += "<table border='2px'><tr>"
            tab_text += TableMarkUp
            tab_text += '</table></body></html>';

            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");

            if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) {
                if (window.navigator.msSaveBlob) {
                    var blob = new Blob([tab_text], {
                        type: "application/csv;charset=utf-8;"
                    });
                    navigator.msSaveBlob(blob, 'List Of Feeback_' + dateExport +'_.xls');
                }
            } else {
                $('#ExcelReportAnchor')[0].click()
            }
        })
    }

    $($("#ExcelReportAnchor")[0]).click(function () {
        $('#ExcelReportAnchor').attr('href', data_type + ', ' + encodeURIComponent(tab_text));
        $('#ExcelReportAnchor').attr('download', 'List Of Feeback_' + dateExport +'_.xls');
    });
