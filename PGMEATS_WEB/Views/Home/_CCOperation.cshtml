<script type="text/javascript">

    $('#chkSAPVAOut').change(function () {
        if (document.getElementById("chkSAPVAOut").checked == true) {
            document.getElementById("chkSAPVAOut").value = "VA";
        }
        else {
            document.getElementById("chkSAPVAOut").value = "";
        }
    });

    $('#chkSAPRunning').change(function () {
        if (document.getElementById("chkSAPRunning").checked == true) {
            document.getElementById("chkSAPRunning").value = "Running";
        }
        else {
            document.getElementById("chkSAPRunning").value = "";
        }
    });

    $('#chkSAPAssy').change(function () {
        if (document.getElementById("chkSAPAssy").checked == true) {
            document.getElementById("chkSAPAssy").value = "Assy";
        }
        else {
            document.getElementById("chkSAPAssy").value = "";
        }
    });

    $('#chkSAPPainting').change(function () {
        if (document.getElementById("chkSAPPainting").checked == true) {
            document.getElementById("chkSAPPainting").value = "Painting";
        }
        else {
            document.getElementById("chkSAPPainting").value = "";
        }
    });



    $('#chkKAPVAOut').change(function () {
        if (document.getElementById("chkKAPVAOut").checked == true) {
            document.getElementById("chkKAPVAOut").value = "VA";
        }
        else {
            document.getElementById("chkKAPVAOut").value = "";
        }
    });

    $('#chkKAPRunning').change(function () {
        if (document.getElementById("chkKAPRunning").checked == true) {
            document.getElementById("chkKAPRunning").value = "Running";
        }
        else {
            document.getElementById("chkKAPRunning").value = "";
        }
    });

    $('#chkKAPAssy').change(function () {
        if (document.getElementById("chkKAPAssy").checked == true) {
            document.getElementById("chkKAPAssy").value = "Assy";
        }
        else {
            document.getElementById("chkKAPAssy").value = "";
        }
    });

    $('#chkKAPPainting').change(function () {
        if (document.getElementById("chkKAPPainting").checked == true) {
            document.getElementById("chkKAPPainting").value = "Painting";
        }
        else {
            document.getElementById("chkKAPPainting").value = "";
        }
    });



    $("#CalcDelMonitoring").click(function () {
        //==============================
        //Get Value Checkbox
        //==============================
        var PlantIDSAP;
        var PlantIDKAP;

        var SAPVAOut = document.getElementById("chkSAPVAOut").value;
        var SAPRunning = document.getElementById("chkSAPRunning").value;
        var SAPAssy = document.getElementById("chkSAPAssy").value;
        var SAPPainting = document.getElementById("chkSAPPainting").value;

        if (SAPVAOut != "" || SAPRunning != "" || SAPAssy != "" || SAPPainting != "") {
            PlantIDSAP = "1";
        }
        else {
            PlantIDSAP = "0";
        }

        var KAPVAOut = document.getElementById("chkKAPVAOut").value;
        var KAPRunning = document.getElementById("chkKAPRunning").value;
        var KAPAssy = document.getElementById("chkKAPAssy").value;
        var KAPPainting = document.getElementById("chkKAPPainting").value;

        if (KAPVAOut != "" || KAPRunning != "" || KAPAssy != "" || KAPPainting != "") {
            PlantIDKAP = "1";
        }
        else {
            PlantIDKAP = "0";
        }

        if (PlantIDSAP == "0" && PlantIDKAP == "0") {
            toastr.warning("Please Select Data!", 'Warning', { timeOut: 10000, "closeButton": true });
        }
        else {
            $("#ModalStartDelMon").modal("show");
        }
        
    })

    $("#StartYesDelMon").click(function () {
        $("#ModalStartDelMon").modal("hide");
         waitingDialog.show("Please wait Start the Calculation data...");

        //==============================
        //Get Value Checkbox
        //==============================
        var PlantIDSAP;
        var PlantIDKAP;

        var SAPVAOut = document.getElementById("chkSAPVAOut").value;
        var SAPRunning = document.getElementById("chkSAPRunning").value;
        var SAPAssy = document.getElementById("chkSAPAssy").value;
        var SAPPainting = document.getElementById("chkSAPPainting").value;

        var KAPVAOut = document.getElementById("chkKAPVAOut").value;
        var KAPRunning = document.getElementById("chkKAPRunning").value;
        var KAPAssy = document.getElementById("chkKAPAssy").value;
        var KAPPainting = document.getElementById("chkKAPPainting").value;

        if (SAPVAOut != "" || SAPRunning != "" || SAPAssy != "" || SAPPainting != "") {
            PlantIDSAP = "1";
        }

        if (KAPVAOut != "" || KAPRunning != "" || KAPAssy != "" || KAPPainting != "") {
            PlantIDKAP = "1";
        }

        //==============================
        //Insert to Database
        //==============================
        $.ajax({
            type: "Post",
            url: "@Url.Action("CalcDelMonitoring", "Home")",
            data: {
                SAPPlantID: PlantIDSAP, SAPVAOut: SAPVAOut, SAPRunning: SAPRunning, SAPAssy: SAPAssy, SAPPainting: SAPPainting,
                KAPPlantID: PlantIDKAP, KAPVAOut: KAPVAOut, KAPRunning: KAPRunning, KAPAssy: KAPAssy, KAPPainting: KAPPainting
            },
            dataType: "json",
            success: function (data) {
                if (!data.Success) {
                    toastr.warning(data.ErrorMessage, 'Warning', { timeOut: 3000, "closeButton": true });
                } else {
                    toastr.success("Data Calculation Delivery Monitoring Successfully!", 'Success', { timeOut: 3000, "closeButton": true });
                }
            },
            error: function (jqXHR, error, errorThrown) {
                toastr.error(jqXHR.status + " : " + jqXHR.responseText, 'Error', { timeOut: 3000, "closeButton": true });
            }
        })
        
        setTimeout(function () {
            waitingDialog.hide();
        }, 1000);
    });

    $("#CalcVolumeAdjust").click(function () {
        //==============================
        //Get Value Checkbox
        //==============================
        var orderArrPriok = [];
        orderArrPriok.length = 0;

        var orderArrPatimban = [];
        orderArrPatimban.length = 0;

        $.each($("#detailsTablePriok tbody tr"), function () {
            if ($(this).find('input:eq(0)').prop('checked')) {
                orderArrPriok.push({
                    PriokVAParam: $(this).find('input:eq(0)').attr('id')
                });
            }
        });

        $.each($("#detailsTablePatimban tbody tr"), function () {
            if ($(this).find('input:eq(0)').prop('checked')) {
                orderArrPatimban.push({
                    PatimbanVAParam: $(this).find('input:eq(0)').attr('id')
                });
            }
        });

        var VAPriok = orderArrPriok;
        var VAPatimban = orderArrPatimban;

        if (VAPriok.length == 0 && VAPatimban == 0) {
            toastr.warning("Please Select Data!", 'Warning', { timeOut: 10000, "closeButton": true });
        }
        else {
            $("#ModalStartVolAdjust").modal("show");
        }
    })

    $("#StartYesVolAdjust").click(function () {
        $("#ModalStartVolAdjust").modal("hide");
        waitingDialog.show("Please wait Start the Calculation data...");
                
        //==============================
        //Get Value Checkbox
        //==============================
        var orderArrPriok = [];
        orderArrPriok.length = 0;

        var orderArrPatimban = [];
        orderArrPatimban.length = 0;

        $.each($("#detailsTablePriok tbody tr"), function () {
            if ($(this).find('input:eq(0)').prop('checked')) {
                orderArrPriok.push({
                    PriokVAParam: $(this).find('input:eq(0)').attr('id')
                });
            }
        });

        $.each($("#detailsTablePatimban tbody tr"), function () {
            if ($(this).find('input:eq(0)').prop('checked')) {
                orderArrPatimban.push({
                    PatimbanVAParam: $(this).find('input:eq(0)').attr('id')
                });
            }
        });

        var VAPriok = orderArrPriok;
        var VAPatimban = orderArrPatimban;

        var dataPriok = "";
        var ndataPriok = "";

        for (var i = 0; i < VAPriok.length; i++) {
            var n = VAPriok.length - 1;
                if (i == n) {
                    dataPriok = VAPriok[i].PriokVAParam;
                } else {
                    dataPriok = VAPriok[i].PriokVAParam + ",";
                }
                ndataPriok = ndataPriok + dataPriok;
        }

        var dataPatimban = "";
        var ndataPatimban = "";
        for (var x = 0; x < VAPatimban.length; x++) {
            var n = VAPatimban.length - 1;
            if (x == n) {
                dataPatimban = VAPatimban[x].PatimbanVAParam;
            } else {
                dataPatimban = VAPatimban[x].PatimbanVAParam + ",";
            }
            ndataPatimban = ndataPatimban + dataPatimban;
        }

        //==============================
        //Insert to Database
        //==============================
        $.ajax({
            type: "Post",
            url: "@Url.Action("CalcVolumeAdjust", "Home")",
            data: { sdataPriok: ndataPriok, sdataPatimban: ndataPatimban },
            dataType: "json",
            success: function (data) {
                if (!data.Success) {
                    toastr.warning(data.ErrorMessage, 'Warning', { timeOut: 3000, "closeButton": true });
                } else {
                    toastr.success("Data Calculation Volume Adjustment Successfully!", 'Success', { timeOut: 3000, "closeButton": true });
                }
            },
            error: function (jqXHR, error, errorThrown) {
                toastr.error(jqXHR.status + " : " + jqXHR.responseText, 'Error', { timeOut: 3000, "closeButton": true });
            }
        })

        setTimeout(function () {
            waitingDialog.hide();
        }, 1000);
    });
</script>