<div class="row" style="background-color:#f2f2f2; margin: 5px 1px 0px 1px">
    <h2 style="font-family:Tahoma;font-weight:900;font-size:32px;font-style:normal;color:#464653;margin: 15px 15px">Job Assignment</h2>
    <table style="width:100%; height:100%">

        @if (ViewBag.AndonJobAssigmentCount != null)
        {
            foreach (var items in ViewBag.AndonJobAssigmentCount)
            {

                <tr>
                    <td style="width:5%"></td>
                    <td style="width:20%">
                        <table style="width:100%; background-color:#464653; border-radius: 15px 15px">
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:#fff">
                                        Active Job
                                    </span>
                                </td>
                                <td style="text-align:center; width:45%" rowspan="2">
                                    <table style="width:100%; background-color:black; border-radius: 15px 15px">
                                        <tr>
                                            <td>
                                                <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:white">
                                                    Move
                                                </span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:white">
                                                    Yard
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td style="width:5%"></td>
                            </tr>
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:34px;font-weight:900;color:#fff">
                                        @items.CountMoveYard
                                    </span>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td style="width:3%"></td>
                    <td style="width:20%">
                        <table style="width:100%; background-color:#fff; border-radius: 15px 15px">
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:#464653">
                                        Active Job
                                    </span>
                                </td>
                                <td style="text-align:center; width:45%" rowspan="2">
                                    <table style="width:100%; background-color:#464653; border-radius: 15px 15px">
                                        <tr>
                                            <td rowspan="2">
                                                <span style="font-family:'Segoe UI';font-size:40px;font-weight:900;color:#fff">
                                                    VA
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td style="width:5%"></td>
                            </tr>
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:34px;font-weight:900;color:#464653">
                                        @items.CountVA
                                    </span>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td style="width:4%"></td>
                    <td style="width:20%">
                        <table style="width:100%; background-color:#ffcc00; border-radius: 15px 15px">
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:#464653">
                                        Active Job
                                    </span>
                                </td>
                                <td style="text-align:center; width:45%" rowspan="2">
                                    <table style="width:100%; background-color:#464653; border-radius: 15px 15px">
                                        <tr>
                                            <td rowspan="2">
                                                <span style="font-family:'Segoe UI';font-size:40px;font-weight:900;color:#fff">
                                                    Yard
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td style="width:5%"></td>
                            </tr>
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:34px;font-weight:900;color:#464653">
                                        @items.CountYard
                                    </span>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td style="width:3%"></td>
                    <td style="width:20%">
                        <table style="width:100%; background-color:#ff6666; border-radius: 15px 15px">
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:20px;font-weight:900;color:#464653">
                                        Active Job
                                    </span>
                                </td>
                                <td style="text-align:center; width:45%" rowspan="2">
                                    <table style="width:100%; background-color:#464653; border-radius: 15px 15px">
                                        <tr>
                                            <td rowspan="2">
                                                <span style="font-family:'Segoe UI';font-size:40px;font-weight:900;color:#fff">
                                                    QC
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td style="width:5%"></td>
                            </tr>
                            <tr>
                                <td style="text-align:center; width:50%">
                                    <span style="font-family:'Segoe UI';font-size:34px;font-weight:900;color:#464653">
                                        @items.CountQC
                                    </span>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td style="width:5%"></td>
                </tr>

            }
        }
    </table>
    <br />
    <div style="margin:0px 10px 0px 10px">
        <table style="width:100%; height:100%; background-color:#464653">
            <tr>
                <th rowspan="2" style="background-color:#f2f2f2; width: 10%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">VIN</th>
                <th rowspan="2" style="background-color:#f2f2f2; width: 10%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Destinations</th>
                <th rowspan="2" style="background-color:#f2f2f2; width: 10%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">ETD</th>
                <th rowspan="2" style="background-color:#f2f2f2; width: 14%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Man Power</th>
                <th colspan="3" style="background-color:#f2f2f2; width: 21%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">From</th>
                <th rowspan="2" style="background-color:#f2f2f2; width: 1%"></th>
                <th colspan="3" style="background-color:#f2f2f2; width: 19%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">To</th>
                <th colspan="2" style="background-color:#f2f2f2; width: 15%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Start</th>
            </tr>
            <tr>
                <th style="background-color:#f2f2f2; width: 8%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Area</th>
                <th style="background-color:#f2f2f2; width: 9%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Line</th>
                <th style="background-color:#f2f2f2; width: 5%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Slot</th>
                <th style="background-color:#f2f2f2; width: 7%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Area</th>
                <th style="background-color:#f2f2f2; width: 7%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Line</th>
                <th style="background-color:#f2f2f2; width: 5%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Slot</th>
                <th style="background-color:#f2f2f2; width: 9%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Date</th>
                <th style="background-color:#f2f2f2; width: 6%; vertical-align: middle; text-align: center; color: #464653; flex-wrap: wrap; font-family: 'Segoe UI'; font-size: 17px; font-weight:900">Time</th>
            </tr>

            @if (ViewBag.AndonJobAssigment != null)
            {
                foreach (var items in ViewBag.AndonJobAssigment)
                {

                    <tr style="height:30px">
                        <td style="background-color:#464653; color:#fff; font-size:12pt; font-weight:800; padding-left:10px" align="center">@items.VIN</td>
                        <td style="background-color:#464653; color:#fff; font-size:12pt; font-weight:800" align="center">@items.Destination</td>
                        <td style="background-color:#464653; color:#fff; font-size:12pt; font-weight:800" align="center">@items.ETD</td>
                        <td style="background-color:#464653; color:#fff; font-size:12pt; font-weight:800" align="center">@items.ManPower</td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.FromArea == "YARD" && items.ToArea != "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.FromArea
                                        </td>
                                    }
                                    else if (items.FromArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.FromArea
                                        </td>
                                    }
                                    else if (items.FromArea == "YARD" && items.ToArea == "YARD")
                                    {
                                        <td style="background-color:black;color:white; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.FromArea
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.FromArea
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.FromArea == "YARD" && items.ToArea != "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.FromLine
                                        </td>
                                    }
                                    else if (items.FromArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.FromLine
                                        </td>
                                    }
                                    else if (items.FromArea == "YARD" && items.ToArea == "YARD")
                                    {
                                        <td style="background-color:black;color:white; font-size:12pt; font-weight:800" align="center">
                                            @items.FromLine
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.FromLine
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.FromArea == "YARD" && items.ToArea != "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.FromSlot
                                        </td>
                                    }
                                    else if (items.FromArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.FromSlot
                                        </td>
                                    }
                                    else if (items.FromArea == "YARD" && items.ToArea == "YARD")
                                    {
                                        <td style="background-color:black;color:white; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.FromSlot
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.FromSlot
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td></td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.ToArea == "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.ToArea
                                        </td>
                                    }
                                    else if (items.ToArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.ToArea
                                        </td>
                                    }
                                    else if (items.ToArea == "SL")
                                    {
                                        <td style="background-color:aqua;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.ToArea
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800; border-top-left-radius:8px; border-bottom-left-radius:8px" align="center">
                                            @items.ToArea
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.ToArea == "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.ToLine
                                        </td>
                                    }
                                    else if (items.ToArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.ToLine
                                        </td>
                                    }
                                    else if (items.ToArea == "SL")
                                    {
                                        <td style="background-color:aqua;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.ToLine
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800" align="center">
                                            @items.ToLine
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td style="background-color:#464653">
                            <table style="width:100%; height:23px">
                                <tr>
                                    @if (items.ToArea == "YARD")
                                    {
                                        <td style="background-color:#ffcc00;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.ToSlot
                                        </td>
                                    }
                                    else if (items.ToArea == "QC")
                                    {
                                        <td style="background-color:#ff6666;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.ToSlot
                                        </td>
                                    }
                                    else if (items.ToArea == "SL")
                                    {
                                        <td style="background-color:aqua;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.ToSlot
                                        </td>
                                    }
                                    else
                                    {
                                        <td style="background-color:#fff;color:#464653; font-size:12pt; font-weight:800; border-top-right-radius:8px; border-bottom-right-radius:8px" align="center">
                                            @items.ToSlot
                                        </td>
                                    }
                                </tr>
                            </table>
                        </td>
                        <td style="background-color:#464653; color:#ffcc00; font-size:12pt; font-weight:800" align="center">@items.StartDate</td>
                        <td style="background-color:#464653; color:#ffcc00; font-size:12pt; font-weight:800" align="center">@items.StartTime</td>
                    </tr>
                    <tr style="background-color:#f2f2f2; height:3px">
                        <td colspan="22"></td>
                    </tr>
                }
             }
        </table>
    </div>
</div>